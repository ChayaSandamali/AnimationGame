<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.ninelights.animationgame" >
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FireAnimation"
            android:label="@string/title_activity_fire_animation" >
        </activity>
        <activity
            android:name=".SnowAnimation"
            android:label="@string/title_activity_snow_animation" >
        </activity>
        <activity
            android:name=".TsunamiAnimation"
            android:label="@string/title_activity_tsunami_animation" >
        </activity>
        <activity
            android:name=".CrackAnimation"
            android:label="@string/title_activity_crack_animation" >
        </activity>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />
    </application>

</manifest>
